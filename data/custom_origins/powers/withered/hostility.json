{
	"type": "origins:multiple",
	"recruit":
	{
		"type": "origins:action_over_time",
		"entity_action":
		{
			"type": "origins:and",
			"actions":
			[
				{
					"type": "origins:execute_command",
					"command": "team add soulskeleton",
					"permission_level": 4
				},
				{
					"type": "origins:execute_command",
					"command": "team join soulskeleton @e[type=minecraft:wither_skeleton]",
					"permission_level": 4
				},
				{
					"type": "origins:execute_command",
					"command": "team join soulskeleton @e[type=minecraft:blaze]",
					"permission_level": 4
				},
				{
					"type": "origins:execute_command",
					"command": "team join soulskeleton @s",
					"permission_level": 4
				}
			]
		},
		"interval": 600
	},
	"retaliate":
	{
		"type": "origins:target_action_on_hit",
		"entity_action":
		{
			"type": "origins:execute_command",
			"command": "team empty soulskeleton",
			"permission_level": 4
		},
		"cooldown": 3,
		"hud_render":
		{
			"should_render": false
		},
		"condition":{
			"type": "origins:or",
			"conditions": [
				{
					"type": "origins:entity_type",
					"entity_type": "minecraft:wither_skeleton"
				},
				{
					"type": "origins:entity_type",
					"entity_type": "minecraft:blaze"
				}
			]
		}
	},
	"name": "Wither Skeleton Brothers",
	"description": "Thanks to your withered nature, Blazes and Wither Skeletons are not aggressive, unless you attack them."
}